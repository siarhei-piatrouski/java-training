<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="webProject" class="by.training.andersen.beans.Project">
        <constructor-arg ref="javaProjectManager"/>
        <constructor-arg>
            <bean class="by.training.andersen.beans.JavaDeveloper"/>
        </constructor-arg>
    </bean>

    <bean id="javaProjectManager" class="by.training.andersen.beans.JavaProjectManager"/>

</beans>